{
    "python.linting.pylintEnabled": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": {"when": "$(basename).py"}, 
        "**/*.sqlite3": {"when": "$(basename).sqlite3"}, 
    }
}